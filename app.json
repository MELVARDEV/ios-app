{
  "expo": {
    "name": "Exory",
    "slug": "Exory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/exory-logo512x512.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.melvar.Exory"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.melvar.Exory"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
